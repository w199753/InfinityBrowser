<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <IntermediateOutputPath>../../../Intermediate/Game/iOS/</IntermediateOutputPath>
    <Platforms>ARM64</Platforms>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFramework>net6.0-ios12.0</TargetFramework>
    <OutputType>Exe</OutputType>
    <Nullable>enable</Nullable>
    <ImplicitUsings>true</ImplicitUsings>
    <SupportedOSPlatformVersion>11.2</SupportedOSPlatformVersion>
    <RootNamespace>InfinityEngine.Game</RootNamespace>
    <BaseOutputPath>$(ProjectDir)../../../Binaries/Game/iOS/</BaseOutputPath>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\Core\Infinity.Core.csproj" />
    <ProjectReference Include="..\..\Engine\Infinity.Engine.csproj" />
    <ProjectReference Include="..\..\Graphics\Infinity.Graphics.csproj" />
    <ProjectReference Include="..\..\Rendering\Infinity.Rendering.csproj" />
    <ProjectReference Include="..\..\Window\Infinity.Windowing.csproj" />
  </ItemGroup>
</Project>
